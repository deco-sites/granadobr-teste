{
  "name": "Template Landing Page",
  "path": "/granado/template-landing-page",
  "sections": [
    {
      "type": "website",
      "__resolveType": "website/sections/Seo/Seo.tsx",
      "titleTemplate": "%s | Deco.cx",
      "descriptionTemplate": "%s | Deco.cx",
      "canonical": "https://www.granado.com.br/granado/template-landing-page"
    },
    {
      "__resolveType": "Header Global"
    },
    {
      "page": {
        "urlKey": "campanhas/banho-bebe",
        "pageSize": 36,
        "customFields": {
          "active": true,
          "overrideList": [
            "tag__phebo"
          ]
        },
        "__resolveType": "magento/loaders/product/listingPage.ts",
        "categoryProps": {
          "filters": [],
          "sortOptions": {
            "order": "ASC",
            "sortBy": {
              "value": "name"
            }
          }
        }
      },
      "layout": {
        "format": "Pagination",
        "columns": {
          "mobile": 2,
          "desktop": 4
        },
        "variant": "aside"
      },
      "cardLayout": {
        "layout": {
          "hide": {
            "cta": true,
            "discount": true,
            "favoriteIcon": true
          },
          "basics": {
            "oldPriceSize": "Small",
            "contentAlignment": "Center"
          },
          "onMouseOver": {
            "card": "None",
            "image": "Change image"
          },
          "elementsPositions": {
            "skuSelector": "Top",
            "favoriteIcon": "Top right"
          }
        },
        "__resolveType": "site/loaders/Layouts/ProductCard.tsx"
      },
      "__resolveType": "site/sections/Product/SearchResult.tsx"
    },
    {
      "__resolveType": "Footer - 01"
    }
  ],
  "__resolveType": "website/pages/Page.tsx"
}